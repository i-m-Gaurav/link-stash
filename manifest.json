{
  "manifest_version": 3,
  "name": "Website Saver",
  "version": "1.0",
  "description": "Save websites to a custom list for later viewing",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ]
} 